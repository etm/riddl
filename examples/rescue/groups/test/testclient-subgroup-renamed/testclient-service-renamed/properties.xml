<?xml version="1.0"?>
<service-details xmlns="http://rescue.org/ns/service/0.2">
  <properties>
    <longitude>4711</longitude>
    <latitude>0815</latitude>
  </properties>
  <operations>
    <query uri="super-kino.at/film" http-method="get">
      <input-message-extension name="op" value="search"/>
    </query>
    <invoke uri="super-kino/film" http-method="post">
      <pre-call uri="super-kino.at/login" http-method="get">
        <call-input-message>
          <parameter name="username"/>
          <parameter name="password"/>
        </call-input-message>
        <call-output-message>
          <parameter name="login-id"/>
        </call-output-message>
      </pre-call>
      <input-message-extension name="op" value="book"/>
    </invoke>
    <undo uri="super-kino/reservations/{reservation-id}" http-method="delete">
      <output-message-extension name="survey-id"/>
      <post-call uri="super-kino.at/survey" http-method="post">
        <call-input-message>
          <parameter name="survey-id"/>
        </call-input-message>
      </post-call>
    </undo>
  </operations>
</service-details>
