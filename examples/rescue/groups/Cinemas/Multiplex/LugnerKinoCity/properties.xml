<?xml version="1.0"?>

<service-details>
  <properties>
    <longitude>4711</longitude>
    <latitude>0815</latitude>
  </properties>
  <operations>
    <read uri="super-kino.at/filme" http-method="get">
      <input name="op" value="search"/>
    </read>
    <create uri="super-kino/fimlme" http-method="post">
      <input name="op" value="book"/>
      <output name="suvey-id"/>
      <pre uri="super-kino.at/login" http-method="post">
        <input name="username"/>
        <input name="password"/>
        <output name="login-id"/>
      </pre>
      <post uri="super-kino.at/survey" http-method="post">
        <input name="survey-id"/>
      </post>
    </create>
    <delete uri="super-kino/reservations/{reservation-id}" http-method="delete">
      <pre uri="super-kino.at/login" http-method="post">
        <input name="user-name"/>
        <input name="password"/>
        <output name="login-id"/>
      </pre>
    </delete>
  </operations>
</service-details>
