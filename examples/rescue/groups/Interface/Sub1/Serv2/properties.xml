<?xml version="1.0"?>

<service-description xmlns="http://rescue.org/ns/service/0.2"
                 xmlns:xsl="http://www.w3.org/1999/XSL/Transform">
  <properties>
  </properties>
  <operations xmlns="http://rescue.org/ns/controlflow/0.2">
    <op1>
      <endpoints>
        <ep>http://www.pri.univie.ac.at/~mangler/services/timeout.php</ep>
      </endpoints>
      <context-variables>
        <wait>5</wait>
      </context-variables>
      <execute>
        <call id="s2_op1" endpoint="ep" http-method="post">
          <input name="timeout" variable="wait"/>
        </call>
        <call id="s2_op1_ext" endpoint="ep" http-method="post">
          <input name="timeout" message-parameter="pi1"/>
          <output name="s2_out" message-parameter="po1"/>
        </call>
      </execute>
      <compensate/>
      <undo/>
      <redo/>
      <suspend/>
      <abort/>
    </op1>
    <op2>
      <endpoints>
      </endpoints>
      <context-variables>
      </context-variables>
      <execute>
        <call id="s2_op2" endpoint="ep" http-method="post">
          <input name="timeout" message-parameter="pi2"/>
        </call>
      </execute>
      <compensate/>
      <undo/>
      <redo/>
      <suspend/>
      <abort/>
    </op2>
  </operations>
</service-description>
