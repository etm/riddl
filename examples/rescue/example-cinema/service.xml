<?xml version="1.0"?>

<service-details>
  <properties>
    <longitude>4711</longitude>
    <latitude>0815</latitude>
  </properties>
  <operations>
    <query uri="super-kino.at/film" http-method="get">
      <input name="op" value="serach"/>
    </query>
    <invoke uri="super-kino/film" http-method="post">
      <input name="op" value="book"/>
      <pre uri="super-kino.at/login" http-method="get">
        <input name="username"/>
        <input name="password"/>
        <output name="login-id"/>
      </pre>
    </invoke>
    <undo uri="super-kino/reservations/{reservation-id}" http-method="delete">
      <output name="survey-id"/>
      <post uri="super-kino.at/survey" http-method="post">
        <input name="survey-id"/>
      </post>
    </undo>
  </operations>
</service-details>
