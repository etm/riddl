<?xml version="1.0"?>
<properties>
  <static>
    <element name="bool">
      <caption lang="en">Boolean</caption>
      <caption lang="de">Ja/Nein</caption>
      <data type="boolean"/>
      </element>
  </static>
  <dynamic>
    <queryInput>
      <element name="int1to10">
        <caption lang="en">Int (1 to 10)</caption>
        <caption lang="de">Int (1 bis 10)</caption>
        <data type="positiveInteger">
          <param name="minInclusive">1</param>
          <param name="maxInclusive">10</param>
        </data>
      </element>
      <element name="date">
        <caption lang="en">Date</caption>
        <caption lang="de">Datum</caption>
        <data type="date"/>
      </element>
      <element name="time">
        <caption lang="en">Time</caption>
        <caption lang="de">Zeit</caption>
        <data type="time"/>
      </element>
      <element name="string">
        <caption lang="en">String</caption>
        <caption lang="de">Zeichenkette</caption>
        <data type="string"/>
      </element>
    </queryInput>

    <queryOutput>
      <element name="bool">
        <caption lang="en">Boolean</caption>
        <caption lang="de">Ja/Nein</caption>
        <data type="boolean"/>
      </element>
    </queryOutput>

    <invokeInput>
      <element name="bool">
        <caption lang="en">Boolean</caption>
        <caption lang="de">Ja/Nein</caption>
        <data type="boolean"/>
      </element>
    </invokeInput>

    <invokeOutput>
      <element name="bool">
        <caption lang="en">Boolean</caption>
        <caption lang="de">Ja/Nein</caption>
        <data type="boolean"/>
      </element>
    </invokeOutput>
  </dynamic>
</properties>
