<!-- Adress: BEGIN -->
    <element name="street">
      <caption lang="en">Street</caption>
      <caption lang="de">Straße</caption>
      <data type="string"/>
    </element>
    <element name="housenumber">
      <caption lang="en">House number</caption>
      <caption lang="de">Hausnummer</caption>
      <data type="positiveInteger"/>
    </element>
    <element name="zip">
      <caption lang="en">ZIP-Code</caption>
      <caption lang="de">Postleitzahl</caption>
      <data type="positiveInteger"/>
    </element>
    <element name="city">
      <caption lang="en">City</caption>
      <caption lang="de">Stadt</caption>
      <data type="string"/>
    </element>
    <element name="state">
      <caption lang="en">State</caption>
      <caption lang="de">Land</caption>
      <data type="string"/>
    </element>
<!-- Adress: END -->
    <element name="aircondition">
      <caption lang="en">Aircondition</caption>
      <caption lang="de">Klimanalage</caption>
      <data type="boolean"/>
    </element>
    <element name="restaurants">
      <caption lang="en">Restaurants</caption>
      <caption lang="de">Restaurantbetriebe</caption>
      <data type="boolean"/>
    </element>
    <element name="roomservice">
      <caption lang="en">Room service</caption>
      <caption lang="de">Zimmerservice</caption>
      <data type="boolean"/>
    </element>
    <element name="carpark">
      <caption lang="en">Parkingspace</caption>
      <caption lang="de">Parkplatz</caption>
      <data type="boolean"/>
    </element>
    <element name="shuttleservice">
      <caption lang="en">Shuttle service</caption>
      <caption lang="de">Pendelbetrieb</caption>
      <data type="boolean"/>
    </element>

  </static>
  <dynamic>
    <queryInput>
      <element name="arraivel">
        <caption lang="en">Arravial</caption>
        <caption lang="de">Ankunft</caption>
        <data type="date"/>
      </element>
      <element name="departure">
        <caption lang="en">Departure</caption>
        <caption lang="de">Abreise</caption>
        <data type="date"/>
      </element>
      <element name="numberOfPersons">
        <caption lang="en">Number of persons</caption>
        <caption lang="de">Personenanzhal</caption>
        <data type="positiveInteger">
          <param name="minInclusive">1</param>
          <param name="maxInclusive">10</param>
        </data>
      </element>
    </queryInput>

    <queryOutput>
      <element name="availability">
        <caption lang="en">Availability</caption>
        <caption lang="de">Verfügbarkeit</caption>
        <data type="boolean"/>
      </element>
      <element name="price">
        <caption lang="en">Price per person</caption>
        <caption lang="de">Preis pro Person</caption>
        <data type="float"/>
      </element>
    </queryOutput>

    <invokeInput>
      <element name="arraivel">
        <caption lang="en">Arravial</caption>
        <caption lang="de">Ankunft</caption>
        <data type="date"/>
      </element>
      <element name="departure">
        <caption lang="en">Departure</caption>
        <caption lang="de">Abreise</caption>
        <data type="date"/>
      </element>
      <element name="numberOfPersons">
        <caption lang="en">Departure</caption>
        <caption lang="de">Abreise</caption>
        <data type="positiveInteger">
          <param name="minInclusive">1</param>
          <param name="maxInclusive">10</param>
        </data>
      </element>
      <element name="bankAccount">
        <caption lang="en">Bank account</caption>
        <caption lang="de">Bankkonto</caption>
        <data type="string"/>
      </element>
    </invokeInput>

    <invokeOutput>
      <element name="reservationID">
        <caption lang="en">Reservation number</caption>
        <caption lang="de">Reservierungsnummer</caption>
        <data type="string"/>
      </element>
    </invokeOutput>


  </dynamic>
</properties>
