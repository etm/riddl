<grammar xmlns="http://relaxng.org/ns/structure/1.0" datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes" ns="http://riddl.org/ns/common-patterns/properties/1.0">
  <start>
    <element name="keys">
      <zeroOrMore>
        <ref name="key"/>
      </zeroOrMore>  
   	</element>
  </start>

  <define name="key">
    <element>
      <anyName/>
      <data type="string"/>
    </element>
  </define>  

</grammar>
